{
  "name": "webpackBase",
  "version": "1.0.0",
  "description": "a webpack base item",
  "author": "xuchangjian <189323200@qq.com>",
  "private": true,
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.vue",
    "*.less"
  ],
  "scripts": {
    "dll": "webpack --config bin/webpack.dll.config",
    "server": "cross-env NODE_ENV=development node bin/dev-server.js",
    "dev": "cross-env NODE_ENV=development webpack --config   bin/webpack.dev.conf.js",
    "build": "cross-env NODE_ENV=testing webpack --config   bin/webpack.test.conf.js",
    "pre": "cross-env NODE_ENV=preproduction webpack --config bin/webpack.pre.conf.js",
    "pro": "cross-env NODE_ENV=production webpack --config bin/webpack.pro.conf.js",
    "test": "cross-env NODE_ENV=production npm_config_report=true webpack --config bin/webpack.test.conf.js  "
  },
  "dependencies": {
    "axios": "^0.18.0",
    "lodash": "^4.17.10",
